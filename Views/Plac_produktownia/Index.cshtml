@model IEnumerable<Projekt2.Models.Plac_produktownia>

@{
    ViewData["Title"] = "Plac buraczany -> Produktownia";
}

<h1>Plac buraczany -&gt Produktownia</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id_dostawy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_partii)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ilosc_burakow_pobrana)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Data_pobrania)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id_dostawy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id_partii)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ilosc_burakow_pobrana)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data_pobrania)
            </td>
            <td>
                    <a class="btn btn-info" asp-action="Details" asp-route-id="@item.Id">Details</a> |
            </td>
        </tr>
}
    </tbody>
</table>
